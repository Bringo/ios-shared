<?xml version="1.0"?>
<buildproducts>
	<product id="SamsClub-iOS">
		<description>SamsClub for iOS</description>
		<git><repo>git@github.com:walmartlabs-samsclub/SamsClub-iOS.git</repo></git>
		<project_file>SamsClub-iOS.xcodeproj</project_file>

		<configurations>
			<configuration><cname>Debug</cname></configuration>
			<configuration><cname>Release</cname><appstore>yes</appstore></configuration>
			<configuration><cname>TestFlight</cname></configuration>
			<configuration><cname>TestFlightRelease</cname></configuration>
		</configurations>

		<schemes>
			<scheme>
				<sname>SamsClub</sname>
				<binary>samsclub</binary>
			</scheme>
		</schemes>

		<entitlement></entitlement>
		<plist>SamsClub-Info.plist</plist>
		<sdk>iphoneos</sdk>
    </product>

	<product id="Walmart-iOS">
		<description>Walmart for iPhone</description>
		<git><repo>git@github.com:walmartlabs-wmusiphone/walmart-ios.git</repo></git>
		<project_file>walmart.xcodeproj</project_file>

		<configurations>
			<configuration><cname>Debug</cname></configuration>
			<configuration><cname>Release</cname></configuration>
			<configuration><cname>Testflight</cname></configuration>
			<configuration><cname>AdHoc</cname></configuration>
			<configuration>
				<cname>Distribution</cname>
				<appstore>yes</appstore>
			</configuration>
		</configurations>

		<schemes>
			<scheme>
				<sname>walmart</sname>
				<binary>walmart</binary>
			</scheme>
		</schemes>

		<entitlement></entitlement>
		<plist>SamsClub-Info.plist</plist>
		<sdk>iphoneos</sdk>
    </product>

	<product id="ASDA-iOS">
		<description>ASDA for iOS</description>
		<git><repo>git@github.com:walmartlabs-asdaios/asda-ios.git</repo></git>
		<project_file>asda.xcodeproj</project_file>

		<configurations>
			<configuration><cname>Debug</cname></configuration>
			<configuration><cname>Release</cname></configuration>
			<configuration><cname>Testflight</cname></configuration>
			<configuration><cname>AdHoc</cname></configuration>
			<configuration>
				<cname>Distribution</cname>
				<appstore>yes</appstore>
			</configuration>
		</configurations>

		<schemes>
			<scheme>
				<sname>ASDA</sname>
				<binary>walmart</binary>
			</scheme>
			<scheme>
				<sname>ASDA - AppStore</sname>
				<binary>asda</binary>
			</scheme>
		</schemes>

		<entitlement></entitlement>
		<plist>SamsClub-Info.plist</plist>
		<sdk>iphoneos</sdk>
    </product>
</buildproducts>
